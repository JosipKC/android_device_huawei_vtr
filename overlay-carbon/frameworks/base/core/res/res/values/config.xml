<resources>
	<!-- Whether a software navigation bar should be shown -->
    <bool name="config_showNavigationBar">true</bool>
    
      <!-- Does the battery LED support multiple colors for notifications? Used to decide if the user can change the colors -->
    <bool name="config_multiColorNotificationLed">true</bool>

    <!-- Is the battery LED intrusive? Used to decide if there should be a disable option -->
    <bool name="config_intrusiveBatteryLed">true</bool>

    <!-- Does the battery LED support multiple colors? Used to decide if the user can change the colors -->
    <bool name="config_multiColorBatteryLed">true</bool>

    <!-- Does the device supports fast charging -->
    <bool name="config_FastChargingLedSupported">true</bool>
    
    <!-- The list absolute paths of jar/apk files containing the device specific handlers,
        delimited by File.pathSeparator, which defaults to ":" on Android  -->
    <string-array name="config_deviceKeyHandlerLibs" translatable="false">/system/priv-app/HuaweiParts/HuaweiParts.apk</string-array>

    <!-- Full qualified name of the class that implements
        com.android.internal.os.DeviceKeyHandler interface. -->
    <string-array name="config_deviceKeyHandlerClasses" translatable="false">org.carbonrom.settings.device.KeyHandler</string-array>
    
</resources>
